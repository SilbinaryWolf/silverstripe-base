{
	"name": "silverstripe-australia/base",
	"description": "Ozzy: the SilverStripe Australia project installer",
	"require": {
		"php": ">=5.3.2",
		"gdmedia/ss-auto-git-ignore": "^0.0.3",
		"silverstripe-australia/build": "~2.0",
		"silverstripe/cms": "~3.5@stable",
		"silverstripe/framework": "~3.5@stable",
		"silverstripe-themes/simple": "*",
		"silverstripe/sqlite3": "1.4.x-dev",
		"silverstripe-australia/ssautesting": "~1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"phpunit/php-code-coverage": "~1.2",
		"phpunit/phpunit-selenium": ">=1.2"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.1.x-dev"
		}
	},
	"scripts": {
		"post-update-cmd": [
			"GDM\\SSAutoGitIgnore\\UpdateScript::Go"
		],
		"post-install-cmd": [
			"GDM\\SSAutoGitIgnore\\UpdateScript::Go"
		]
	},
	"minimum-stability": "dev"
}
